<div class="container">

<h2>ジャンル編集</h2>

<div>
    <%= form_with model: @genre, url: admin_genre_path, local: true do |f| %>
      <div>
        <div>
          <%= f.label "ジャンル名" %>
          <%= f.text_field :name %>
        </div>
        <div>
          <%= f.submit "変更を保存" %>
        </div>
      </div>
    <% end %>
  </div>
</div>