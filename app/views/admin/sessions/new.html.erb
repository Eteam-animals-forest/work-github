<div class="container">
  <div class="row justify-content-center">
    <div class="col-10 py-5 center-block">
    　<h2 class="mb-4">管理者ログイン</h2>
        <table>
　       <%= form_with model:resource, url: admin_session_path do |f| %>
　       <%= alert %>
　       <tr>
           <td><%= f.label :email, "メールアドレス" %></td>
           <td><%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "sample@example.com" %></td>
         </tr>
         <tr>
           <td><%= f.label :password, "パスワード" %></td>
           <td><%= f.password_field :password, autocomplete: "current-password" %></td>
         </tr>
         <tr>
           <td><%= f.submit "ログイン", class: "btn btn-primary" %></td>
         </tr>
         <% end %>
        </table>
       <%= render "admin/shared/links" %>
      </div>
    </div>
 </div>